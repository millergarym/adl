module union03 {

struct Abc<T> {
    T a;
    String b = "abc!!";
};

union A<T> {
    Vector<Int64> B;
    Abc<T> C;
    B a;
};

union B {
    Int64 a;
    Int64 b;
};

struct Ghi {
    A<String> x = {"B": [1,2,3]};
    A<String> y = {"C": {"a": "zyx"}};
    A<String> z = {"a": {"a": 123}};
};

};